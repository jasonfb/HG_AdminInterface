<div class='col'>
  <%= person.name %>
</div>



<div class="col">
  
    <%= form_with url: admin_account_person_path(account,person), method: :delete do |f| %>
      <%= f.submit "Delete".html_safe, data: {confirm: 'Are you sure?'}, class: "delete-person-button btn btn-primary " %>
    <% end %>
  


  

  
  <%= link_to "Edit <i class='fa fa-1x fa-list-alt'></i>".html_safe, edit_admin_account_person_path(account,person), disable_with: "Loading...", class: "edit-person-button btn btn-primary " %>
  
</div>
